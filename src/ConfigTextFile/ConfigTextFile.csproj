<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net5.0;netstandard2.1;netstandard2.0</TargetFrameworks>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <Company />
    <Product>ConfigTextFile</Product>
    <Authors>Michael McBride</Authors>
    <RepositoryUrl>https://github.com/Michmcb/ConfigTextFile</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageTags>config configuration text fluent</PackageTags>
    <PackageProjectUrl>https://github.com/Michmcb/ConfigTextFile</PackageProjectUrl>
    <Description>A .NET library which allows you to read and write plain-text minimal syntax configuration files</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReleaseNotes>- Added a ConfigBuilder, which provides a fluent interface to build up a hierarchy of config elements
- When passing a null for the value of a ConfigStringElement, it no longer coalesces it to string.Empty</PackageReleaseNotes>
    <PackageIcon>icon.png</PackageIcon>
    <Version>1.9.1</Version>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <AssemblyVersion>1.9.1.0</AssemblyVersion>
    <FileVersion>1.9.1.0</FileVersion>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>


  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
